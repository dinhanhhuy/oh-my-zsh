gmb() {
  rawbranchs=$(git branch --merged $1 | egrep -v "(^\*|master|develop)")
  if [[ $2 == "-s" ]]; then
    return 0
  fi
  echo $rawbranchs
}

gdmb() {
  gmb develop -s
  echo "$rawbranchs" | while read each
  do
    rinput "$(tput setaf 0)delete branch$(tput setaf 1) $each$(tput setaf 0)" "skip"
    if [[ $? == 0 ]]; then
      git branch -d $each
      git push origin --delete $each
    fi
  done
}

